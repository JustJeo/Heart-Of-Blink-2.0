<!-- After a player hits the "submit" button -->
<h1>Here's How You Did!</h1>
<h2>Song: "<%= blink.song.songTitle %>"</h2>
<!-- Stretch Goal -->
<!-- <h2>Score:</h2> -->

<div class="answersList">
  <% for(let i = 0; i < 10; i++) { %>
    <div class="answerCard">
      <label><%= blink.lyrics[i].questionMessage %></label>
      <p>The correct answer is: <b></b><%= blink.lyrics[i].questionAnswer %></p>
      <% let answer = `lyric${blink.lyrics[i].id}` %>
      <p>You entered: <%= blink.playerAnswer[answer] %></p>
      <% console.log(blink.lyrics[i].id) %>
    </div>  
  <% } %>
</div>

